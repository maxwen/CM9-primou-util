<?xml version="1.0" encoding="UTF-8"?>
  <manifest>

    <remote name="Lloir"
        fetch="https://github.com/Lloir/"
        revision="refs/heads/ics"/>

    <remote name="maxwen"
        fetch="https://github.com/maxwen/"
        revision="refs/heads/ics-release"/>

    <remove-project name="CyanogenMod/android_hardware_ril" path="hardware/ril"/>
    <project path="hardware/ril"
       remote="maxwen"
       name="android_hardware_ril"/>

    <remove-project name="CyanogenMod/android_packages_apps_Camera" path="packages/apps/Camera"/>
    <project path="packages/apps/Camera"
       remote="maxwen"
       name="android_packages_apps_Camera"/>

    <remove-project name="CyanogenMod/android_frameworks_base" path="frameworks/base"/>
    <project path="frameworks/base"
       remote="maxwen"
       name="android_frameworks_base"/>

    <remove-project name="CyanogenMod/android_prebuilt" path="prebuilt"/>
    <project path="prebuilt"
        revision="ics"
        name="AOKP/prebuilt"/>

    <remove-project name="CyanogenMod/android_hardware_qcom_media" path="hardware/qcom/media"/>
    <project path="hardware/qcom/media"
        remote="maxwen"
        name="android_hardware_qcom_media"/>

    <project name="android_device_primou" 
        path="device/htc/primou" 
        remote="Lloir" />

    <project name="Primou_msm7x30-common" 
        path="device/htc/msm7x30-common" 
        remote="Lloir"/>

    <project name="android_vendor_htc_primou" 
        path="vendor/htc/primou" 
        remote="Lloir" />

  </manifest>
